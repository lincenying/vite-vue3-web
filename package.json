{
  "name": "template-vue3-web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.3",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "del": "sh bash/delete-ts.sh",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build_test": "vite build --mode test --emptyOutDir",
    "build_pre": "vite build --mode staging --emptyOutDir",
    "build_prod": "vite build --emptyOutDir",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@lincy/async-validation": "^1.3.0",
    "@lincy/utils": "^0.6.1",
    "@unhead/vue": "^1.11.16",
    "@unocss/reset": "^65.4.0",
    "@vueuse/core": "^12.4.0",
    "@vueuse/router": "^12.4.0",
    "axios": "^1.7.9",
    "default-passive-events": "^2.0.0",
    "element-plus": "^2.9.3",
    "js-cookie": "^3.0.5",
    "locutus": "^2.0.32",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "^2.3.0",
    "qs": "^6.13.1",
    "store2": "^2.14.4",
    "vue": "^3.5.13",
    "vue-countup-v3": "^1.4.2",
    "vue-router": "^4.5.0",
    "vue3-seamless-scroll": "^2.0.1"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.5",
    "@iconify-json/fad": "^1.2.1",
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify-json/vscode-icons": "^1.2.10",
    "@lincy/eslint-config": "^5.4.0",
    "@lincy/stylelint-config": "^2.0.1",
    "@lincy/unocss-base-config": "^2.0.1",
    "@types/locutus": "^0.0.8",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.10.5",
    "@types/polyfill-library": "^3.108.4",
    "@types/qs": "^6.9.17",
    "@unocss/eslint-plugin": "^65.4.0",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue-macros/volar": "^0.30.10",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.3.0",
    "mockjs": "^1.1.0",
    "polyfill-library": "^4.8.0",
    "sass": "^1.83.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unocss": "^65.4.0",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-icons": "^22.0.0",
    "unplugin-vue-components": "^28.0.0",
    "unplugin-vue-macros": "^2.13.8",
    "url-search-params-polyfill": "^8.2.5",
    "vite": "^6.0.7",
    "vite-plugin-inspect": "^10.0.6",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^2.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "postcss",
        "terser",
        "prettier",
        "esbuild"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
