{
  "name": "template-vue3-web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.22.0",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "prepare[-del]": "npx simple-git-hooks",
    "del": "sh bash/delete-ts.sh",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build_test": "vite build --mode test --emptyOutDir",
    "build_pre": "vite build --mode staging --emptyOutDir",
    "build_prod": "vite build --emptyOutDir",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc -b --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "lint-staged": "lint-staged",
    "copy": "deploy_dist vite-vue3-web"
  },
  "dependencies": {
    "@lincy/async-validation": "^1.4.0",
    "@lincy/utils": "^0.7.2",
    "@tanstack/vue-query": "^5.91.2",
    "@unhead/vue": "^2.0.19",
    "@unocss/reset": "^66.5.6",
    "@vueuse/core": "^14.0.0",
    "@vueuse/router": "^14.0.0",
    "alova": "^3.3.4",
    "axios": "^1.13.2",
    "default-passive-events": "^4.0.0",
    "element-plus": "^2.11.8",
    "mitt": "^3.0.1",
    "pinia": "^3.0.4",
    "qs": "^6.14.0",
    "store2": "^2.14.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@iconify-json/carbon": "^1.2.14",
    "@iconify-json/fad": "^1.2.1",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@iconify-json/vscode-icons": "^1.2.35",
    "@lincy/eslint-config": "^6.0.0",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.3.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^24.10.1",
    "@types/polyfill-library": "^3.108.4",
    "@types/qs": "^6.14.0",
    "@unocss/eslint-plugin": "^66.5.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue-macros/volar": "^3.1.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.6",
    "mockjs": "^1.1.0",
    "polyfill-library": "^4.8.0",
    "sass": "^1.94.1",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.25.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.6",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.2",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vue-macros": "^3.1.1",
    "vue-tsc": "^3.1.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "postcss",
        "terser",
        "prettier",
        "esbuild"
      ]
    },
    "onlyBuiltDependencies": [
      "simple-git-hooks",
      "vue-demi"
    ]
  },
  "simple-git-hooks[-del]": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged[-del]": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
